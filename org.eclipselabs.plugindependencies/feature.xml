<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipselabs.plugindependencies"
      label="Plugin Dependencies Explorer"
      version="1.3.3.qualifier"
      provider-name="Andrey Loskutov"
      plugin="org.eclipselabs.plugindependencies.ui">

   <description url="https://github.com/iloveeclipse/plugindependencies/wiki">
      Project provides both command line and Eclipse UI for static package, plugin and feature dependencies analysis (the dependencies are read from MANIFEST.MF and feature.xml files).
