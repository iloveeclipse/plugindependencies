All rights reserved.
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      Copyright (c) 2015 Andrey Loskutov, licensed under the Eclipse Public License 1.0.
   </license>

   <url>
      <update label="Andrey Loskutov plugins site" url="http://andrei.gmxhome.de/eclipse/"/>
      <discovery label="Zip Editor" url="http://zipeditor.sourceforge.net/update/"/>
      <discovery label="FindBugs" url="http://findbugs.cs.umd.edu/eclipse/"/>
      <discovery label="Eclemma Java Code Coverage" url="http://update.eclemma.org/"/>
      <discovery label="Subversive" url="http://download.eclipse.org/technology/subversive/3.0/update-site/"/>
      <discovery label="EGit" url="http://download.eclipse.org/egit/updates/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.ui.ide" version="3.9.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.pde" version="3.9.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.eclipselabs.plugindependencies.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipselabs.plugindependencies.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
